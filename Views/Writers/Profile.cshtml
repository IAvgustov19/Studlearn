@model SW.Frontend.Models.WriterModel
@using System.Globalization;
@{
    ViewBag.Title = "Отзывы о " + Model.Name;
    ViewBag.Description = "Детальное описание \"" + Model.Name + "\". Включая email, адрес сайта, страничку ВКонтакте и размер его левой ноги.";
    ViewBag.Image = Model.Image;
    Layout = "~/Views/Shared/_Layout.cshtml";

    var page = 1;
    int.TryParse(Request["page"] ?? "1", out page);
    if (page != 1)
    {
        ViewBag.Title += " - Страница " + page;
        ViewBag.Description += " - Страница " + page;
    }
}

@section Head{
    @if (page != 1)
    {
        <link rel="canonical" href="https://studlearn.com/writers/profile/@ViewBag.WriterSlug" />
    }
}

@section Styles {
    <link href="~/scripts/plugins/raty/jquery.raty.css" rel="stylesheet" />
    @Html.Raw(ViewBag.MicrodataProductSnippet)

    <style>
        .blog-item .comments {
            padding-top: 0px;
        }

        .media p {
            margin: 0;
        }

        .media, .media .media {
            padding-bottom: 5px;
            padding-top: 5px;
            margin: 0;
        }

        .like {
            cursor: pointer;
        }

            .like:hover {
                font-weight: bold;
            }

        .dislike {
            cursor: pointer;
        }

            .dislike:hover {
                font-weight: bold;
            }
    </style>
}

