    <script>
        $(document).ready(function()
        {
            @if (Model.ShowEdit)
                {
                    @:$('.editable').mouseenter(In).mouseleave(Out);
                }
        });

        function In(){
            $(this).html($(this).html() + "<a style=\"cursor: pointer;\" class=\"edit\"><i class=\"fa fa-pencil\"></i></a>");

            $(this).find('.edit').click(function(e)
            {
                var parent = $(this).closest('.editable');
                e.preventDefault();

                var dataField = parent.data('field');
                var wasHref = false;
                if (parent.is('a'))
                {
                    wasHref = true;
                    parent.replaceWith(function(){
                        return $('<span class="editable" href="'+$(this).attr('href')+'" data-field="' +$(this).data('field')  + '">' + $(this).html() + '</span>');
                    });
                    parent = $('span[data-field=' + dataField + ']');
                }

                parent.off('hover').html('<div class="form-inline" style="display: inline;"><div class="form-group"><div class="input-group input-inline input-medium">' +
                    '<input class="form-control editable-input" type="text" value="' + $(this).closest('.editable').text() + '" >' +
                    '<span class="input-group-btn">' +
                    '<button class="btn green saveeditable" type="button"><i class="fa fa-check"></i></button>'+
                    '</span>' +
                    '</div></div></div>');

                parent.find('.editable-input').click(function(e)
                {
                    e.preventDefault();
                });

                parent.find('.saveeditable').click(function(e)
                {
                    var element = $(this).closest('.editable');
                    if (element.is('a'))
                    {
                        e.preventDefault();
                    }

                    $.ajax({
                        type: 'POST',
                        url: '/api/internal/externalwriters/updatefield',
                        dataType: "json",
                        data: JSON.stringify({
                            WriterId: "@Model.Id",
                            Field: element.data('field'),
                            Value: element.find('input').val()
                        }),
                        contentType: "application/json"
                    }).done(function()
                    {
                        SwCore.showSuccess("Готово", 'Поле успешно обновлено.');
                        var newValue = element.find('input').val();

                        if (wasHref)
                        {
                            parent.replaceWith(function(){
                                return $('<a class="editable" href="'+ newValue +'" data-field="' +$(this).data('field')  + '">' + $(this).html() + '</a>');
                            });
                            element = $('a[data-field=' + dataField + ']');
                        }

                        element.on('mouseenter', In).on('mouseleave', Out).html(newValue);

                    }).fail(function(jqXHR, textStatus) {
                        SwCore.showError("Ошибка сохранения", jqXHR.responseJSON.ExceptionMessage);
                    });

                });
            });
        }

        function Out()
        {
            $(this).find('.edit').remove();
            $(this).html($(this).html());
        }
    </script>

    <script>
        $(document).ready(function () {
            $('.like').click(function () {
                if (!$(this).hasClass('liked')) {
                    if ($(this).parent().find('.dislike').hasClass('liked')) {
                        var opponent = $(this).parent().find('.dislike');
                        opponent.removeClass('bold');
                        opponent.removeClass('liked');
                        var i = parseInt(opponent.find('.like-count').text());
                        opponent.find('.like-count').text(i - 1)
                    }
                    $(this).addClass('bold');
                    $(this).addClass('liked');
                    var i = parseInt($(this).find('.like-count').text());
                    $(this).find('.like-count').text(i + 1);

                    $.ajax({
                        type: 'GET',
                        url: '/api/internal/externalwriters/review/like/' + $(this).data('id') + '/true',
                        dataType: "json"
                    });
                }
                else {
                    $(this).removeClass('bold');
                    $(this).removeClass('liked');
                    var i = parseInt($(this).find('.like-count').text());
                    $(this).find('.like-count').text(i - 1);

                    $.ajax({
                        type: 'GET',
                        url: '/api/internal/externalwriters/review/' + $(this).data('id') + '/delete',
                        dataType: "json"
                    });
                }
            });

            $('.dislike').click(function () {
                if (!$(this).hasClass('liked')) {
                    if ($(this).parent().find('.like').hasClass('liked')) {
                        var opponent = $(this).parent().find('.like');
                        opponent.removeClass('bold');
                        opponent.removeClass('liked');
                        var i = parseInt(opponent.find('.like-count').text());
                        opponent.find('.like-count').text(i - 1)
                    }
                    $(this).addClass('bold');
                    $(this).addClass('liked');
                    var i = parseInt($(this).find('.like-count').text());
                    $(this).find('.like-count').text(i + 1);

                    $.ajax({
                        type: 'GET',
                        url: '/api/internal/externalwriters/review/like/' + $(this).data('id') + '/false',
                        dataType: "json"
                    });
                }
                else {
                    $(this).removeClass('bold');
                    $(this).removeClass('liked');
                    var i = parseInt($(this).find('.like-count').text());
                    $(this).find('.like-count').text(i - 1);

                    $.ajax({
                        type: 'GET',
                        url: '/api/internal/externalwriters/review/' + $(this).data('id') + '/delete',
                        dataType: "json"
                    });
                }
            });
        });
    </script>
}

