    <script type="text/javascript">
        $(document).ready(function () {
            moment.locale('ru');
            SwInit.initDateTime();

            $('.rating-global').raty({
                starType: 'img',
                score: @Model.Rating.ToString(CultureInfo.InvariantCulture),
                @*readOnly: @((string.IsNullOrEmpty(User.Identity.Name)).ToString().ToLower()),*@
                readOnly: true,
                starHalf: '/scripts/plugins/raty/images/star-half-32.png',
                starOff: '/scripts/plugins/raty/images/star-off-32.png',
                starOn: '/scripts/plugins/raty/images/star-on-32.png',
                click: function(number)
                {
                    var url = '/api/internal/externalwriters/' + @Model.Id + '/score/' + number;
                    $.post(url, {}, function(score) {
                        $('#writer-rating').text(score);
                    });
                },
                hints: ['ужасно', 'неуд.', 'удовлетворительно', 'хорошо', 'отлично']
            });

            $('.rating-review').each(function (index)
            {
                $(this).raty({
                    starType: 'img',
                    score: $(this).data('rating'),
                    readOnly: true,
                    starHalf: '/scripts/plugins/raty/images/star-half.png',
                    starOff: '/scripts/plugins/raty/images/star-off.png',
                    starOn: '/scripts/plugins/raty/images/star-on.png',
                    hints: ['ужасно', 'неуд.', 'удовлетворительно', 'хорошо', 'отлично']
                });
            });

            $('.rating-new').raty({
                starType: 'img',
                score: @(Model.NewReview.score == 0 ? Model.UserRating : Model.NewReview.score),
                starHalf: '/scripts/plugins/raty/images/star-half.png',
                starOff: '/scripts/plugins/raty/images/star-off.png',
                starOn: '/scripts/plugins/raty/images/star-on.png',
                hints: ['ужасно', 'неуд.', 'удовлетворительно', 'хорошо', 'отлично']
            });

            $(".review-form").validate({
                ignore: "",
                rules: {
                    Name: { required: true},
                    score: {number:true, min:1, max:5,required: true},
                    Message: {required: true},
                },
                messages: {
                    Name: {required: "*"},
                    score: {required: "*"},
                    Message: {required: "*"}
                },
                highlight: function (element, errorClass, validClass) {
                    $(element).closest('.form-group').removeClass("has-error").addClass("has-error");
                },
                unhighlight: function (element, errorClass, validClass) {
                    $(element).closest('.form-group').removeClass("has-error");
                },
                errorPlacement: function (error, element) {
                    error.appendTo(element.closest('.form-group').find(".help-block"));
                }
            });

            // moderation features
            $('#isMalefactorCheckbox').attr('checked', @Json.Encode(Model.IsMalefactor) );
            $('#isMalefactorCheckbox').change(function(){
                var url = '/api/internal/externalwriters/'+ @Model.Id+'/swap-malefactor';
                $.post(url, {}, function() {})
                    .done(function() { SwCore.showSuccess('Данные успешно обновлены'); })
                    .fail(function() { SwCore.showError('Произошла ошибка на стороне сервера');})
            });
        })
    </script>

    <script>

        $(document).ready(function () {
